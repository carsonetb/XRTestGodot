; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_invalid_track_paths=false

[application]

config/name="xr_test_godot"
run/main_scene="uid://bb0y3mal68jxi"
config/features=PackedStringArray("4.5", "Forward Plus")
run/max_fps=144

[editor_plugins]

enabled=PackedStringArray()

[input]

trigger={
"deadzone": 0.2,
"events": []
}
grip={
"deadzone": 0.2,
"events": []
}

[physics]

3d/run_on_separate_thread=true
3d/physics_engine="Jolt Physics"
jolt_physics_3d/simulation/velocity_steps=20
jolt_physics_3d/simulation/position_steps=8
3d/solver/solver_iterations=32

[rendering]

renderer/rendering_method="mobile"
lights_and_shadows/directional_shadow/size=16384
lights_and_shadows/directional_shadow/soft_shadow_filter_quality=5

